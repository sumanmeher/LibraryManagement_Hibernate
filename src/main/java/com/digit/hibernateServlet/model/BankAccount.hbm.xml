<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.digit.hibernateServlet.bean.AdminDetails" table="admin">
		<id name="acc_no" column="acc_no"></id>
		<property name="pin">
			<column name="pin" />
		</property>
		<property name="amount">
			<column name="amount" />
		</property>
	</class>
</hibernate-mapping>
